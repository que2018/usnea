.container {
  padding: 30px 30px;
  font-family: Arial, Helvetica, sans-serif;
}
.title {
  font-size: 29px;
  font-weight: bold;
  padding-bottom: 15px;
  width: 100%;
}
.ptitle {
  font-weight: bold;
  padding-top: 18px;
  padding-bottom: 10px;
}
.number {
    width: 18px;
    height: 18px;
    border-radius: 500px;
    background: rgb(167, 167, 167);
    float: left;
    color: #ffffff;
    text-align: center;
    font-size: 12px;
    line-height: 18px;
    margin-top: 0px;
    margin-right: 4px;
}
.line {
  background: #e5e5e5;
  margin-top: 30px;
}
.pcontent {
  font-size: 14px;
} 